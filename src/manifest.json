{
  "manifest_version": 2,
  "name": "AntyWyr",
  "description": "Replaces javascript window.open with real links on wykop.pl",
  "homepage_url": "https://github.com/sin2000/antywyr",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://www.wykop.pl/*"],
      "js": ["./substitute.js"]
    }
  ]
}
